{
	"compilerOptions": {
		"alwaysStrict": true,
		"declaration": true,
		"module": "umd",
		"noImplicitAny": false,
		"noStrictGenericChecks": false,
		"outDir": "dist",
		"strict": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": true,
		"sourceMap": false,
		"target": "es5"
	}
}
